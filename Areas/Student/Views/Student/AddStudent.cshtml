@using WebIdentity.Models
@model Student
<form asp-controller="Student" asp-action="AddStudent" method="post">
    <div class="card  col-md-8 p-4 mb-2">
        <div class="form-group row">
            <label asp-for="StudentName" class="col-3"></label>
            <input asp-for="StudentName" class="form-control col-7"/>
        </div>
        
    </div>
    
    
    <button type="submit" class="btn btn-success">Add Student</button>
</form>

