@using WebIdentity.Models
@model IEnumerable<Student>

@{
    ViewData["Title"] = "AllRegistrationClub";
}
<h2>@ViewData["Club"]</h2>
@{
    if (Model.Count() < 1)
    {
        <p>No student registered inr this club yet</p>
    }
    else
    {
        <h3>All Registered Students</h3>
        <table border="1" class="table table-striped table-bordered ">
            <tr>
                <th>Student ID</th>
                <th>Student Name</th>
            </tr>
            @foreach (var student in Model)
            {
                <tr>
                    <td>@student.StudentId</td>
                    <td>@student.StudentName</td>
                </tr>
            }
        </table>
    }
}
<p><a asp-controller="Club" asp-action="AllClub" class="btn btn-info">Back to clubes list</a></p>





